.etc.default.useradd

`ag_ini(
  IniAgent(
    "/etc/default/useradd",
    $[ "options" : [ "ignore_case", "global_values", "flat" ],
       "comments" : [ "^#.*", "^[ \t]*$", ],
       "params" : [
		 $[ "match" : [ "^[ \t]*([^=]*[^ \t=])[ \t]*=[ \t]*(.*[^ \t]|)[ \t]*$" , "%s=%s"],],
       ]
	]
    ))
